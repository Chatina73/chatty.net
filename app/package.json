{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "production": "PORT=443 NODE_ENV=production node server.js",
    "dev": "NODE_ENV=development node server.js",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:dev": "vue-cli-service build --mode development",
    "build:report": "vue-cli-service build --report",
    "lint": "vue-cli-service lint",
    "build-embed:production": "$(npm bin)/browserify --full-paths -t [ $(npm root)/browserify-replace --replace '{ \"from\": \"<BROWSERIFY_REPLACE_MODE>\", \"to\": \"production\" }' --replace '{ \"from\": \"<BROWSERIFY_REPLACE_VENDOR_WEB3>\", \"to\": \"web3.min.js\" }' ] --noparse=$(pwd)/inpage/vendor/web3.js --noparse=$(pwd)/inpage/vendor/web3.min.js ./inpage/embed.js -o public/embed-temp.min.js && $(npm bin)/browserify --full-paths -g uglifyify ./public/embed-temp.min.js > ./public/embed.min.js",
    "build-embed:dev": "$(npm bin)/browserify --full-paths -t [ $(npm root)/browserify-replace --replace '{ \"from\": \"<BROWSERIFY_REPLACE_MODE>\", \"to\": \"development\" }' --replace '{ \"from\": \"<BROWSERIFY_REPLACE_VENDOR_WEB3>\", \"to\": \"web3.js\" }' ] --noparse=$(pwd)/inpage/vendor/web3.js --noparse=$(pwd)/inpage/vendor/web3.min.js ./inpage/embed.js -o public/embed.min.js",
    "build-embed:copy-main": "npm run build-embed && cp ./public/embed.min.js ./public/embed-main.min.js",
    "build-embed:report": "npm run build-embed:production && discify ./public/embed.min.js > disc.html",
    "lint:es": "eslint ./src/views/* --fix",
    "temp": "npm run build-embed:dev && npm run build:dev",
    "test:e2e": "npm run build-embed:dev && npm run build:dev && (npm run dev & pid1=\"$!\"; node ./test/e2e/puppeteer.js && kill $pid1)",
    "test:e2e:headful": "npm run build-embed:dev && npm run build:dev && (npm run dev & pid1=\"$!\"; node ./test/e2e/puppeteer.js false && kill $pid1)",
    "test:codecov-upload": "if [ $CODECOV_TOKEN ]; then nyc report --reporter=text-lcov > coverage.lcov && codecov; fi",
    "test:coverage-codecov": "nyc --reporter=text --reporter=html npm run test:unit && npm run test:codecov-upload",
    "test:coverage-coveralls": "nyc --reporter=text --reporter=html npm run test:unit && npm run test:coveralls-upload",
    "test:coveralls-upload": "if [ $COVERALLS_REPO_TOKEN ]; then nyc report --reporter=text-lcov | coveralls; fi",
    "test:unit": "mocha --exit -r esm -r jsdom-global/register ./test/unit/controllers/*.js ./test/unit/controllers/transactions/*.js"
  },
  "dependencies": {
    "ajv": "^6.7.0",
    "assert": "^1.4.1",
    "backoff": "^2.5.0",
    "browserify": "^16.2.3",
    "browserify-replace": "^1.0.0",
    "debounce": "^1.2.0",
    "dnode": "^1.2.2",
    "dotenv": "^6.2.0",
    "eth-block-tracker": "^4.1.0",
    "eth-json-rpc-filters": "^3.0.1",
    "eth-json-rpc-middleware": "^3.1.7",
    "eth-keyring-controller": "^3.3.1",
    "eth-query": "^2.1.2",
    "ethereumjs-tx": "^1.3.7",
    "ethereumjs-util": "^6.0.0",
    "ethereumjs-vm": "2.5.1",
    "ethereumjs-wallet": "^0.6.3",
    "fast-json-patch": "^2.0.7",
    "human-standard-token-abi": "^2.0.0",
    "json-rpc-engine": "^4.0.0",
    "json-rpc-middleware-stream": "^2.1.0",
    "jsonschema": "^1.2.4",
    "obj-multiplex": "^1.0.0",
    "obs-store": "^3.0.2",
    "percentile": "^1.2.1",
    "pify": "^4.0.1",
    "post-message-stream": "^3.0.0",
    "random-id": "^1.0.3",
    "readable-stream": "^3.1.1",
    "swappable-obj-proxy": "^1.1.0",
    "through2": "^3.0.0",
    "uglifyify": "^5.0.1",
    "vue": "^2.5.22",
    "vue-router": "^3.0.2",
    "vue-script2": "^2.0.3",
    "vuetify": "^1.4.3",
    "vuex": "^3.1.0",
    "vuex-persist": "^2.0.0",
    "web3": "1.0.0-beta.37",
    "web3-provider-engine": "^14.1.0",
    "xtend": "^4.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-service": "^3.3.1",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "codecov": "^3.1.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.1.0",
    "esm": "^3.1.1",
    "eth-phishing-detect": "^1.1.13",
    "ganache-core": "^2.3.3",
    "jsdom": "13.2.0",
    "jsdom-global": "3.0.2",
    "luxon": "^1.10.0",
    "mocha": "^5.2.0",
    "mocha-eslint": "^5.0.0",
    "mocha-jsdom": "^2.0.0",
    "mocha-sinon": "^2.1.0",
    "nock": "^10.0.6",
    "nyc": "^13.1.0",
    "puppeteer": "^1.12.2",
    "sinon": "^5.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-template-compiler": "^2.5.22",
    "vuetify-loader": "^1.1.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "engines": {
    "node": "10.x"
  }
}
